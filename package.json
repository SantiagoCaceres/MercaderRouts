{
  "name": "miprimerapp",
  "productName": "Albion Mercader Routs",
  "version": "1.0.0",
  "description": "",
  "main": "src/main.js",
  "type": "commonjs",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=src/assets/icons/mac/icon.ico --prune=true --out=release-builds",
    "package-win": "electron-packager . Albion-Mercader-Routs --overwrite --asar --platform=win32 --arch=x64 --icon=src/assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Albion Mercader Routs\"",
    "package-win32": "electron-packager . Albion-Mercader-Routs --overwrite --asar --platform=win32 --arch=ia32 --icon=src/assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Albion Mercader Routs\"",
    "package-linux": "electron-packager . Albion-Mercader-Routs --overwrite --asar --platform=linux --arch=x64 --icon=src/assets/icons/png/1024x1024.ico --prune=true --out=release-builds"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "ejs": "^3.1.8",
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "puppeteer": "^19.4.1"
    
  },
  "devDependencies": {
    "nodemon": "^2.0.20",
    "puppeteer": "^19.4.1",
    "express": "^4.18.2"
  }
}
